<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=5.0">

<title><%= typeof title !== 'undefined' ? title : 'Дача Зейна | BSS Community' %></title>
<meta name="description" content="<%= typeof description !== 'undefined' ? description : 'Крупнейшее сообщество по Bee Swarm Simulator. Экономика, биржа акций, ивенты и уникальный бот' %>">
<meta name="theme-color" content="#5865F2">
<meta name="author" content="Dachazeyna Team">

<% if (typeof noIndex !== 'undefined' && noIndex) { %>
    <meta name="robots" content="noindex, nofollow">
<% } else { %>
    <meta name="robots" content="index, follow">
<% } %>

<link rel="canonical" href="https://dachazeyna.com<%= typeof currentPath !== 'undefined' ? currentPath : '' %>">

<meta property="og:type" content="<%= typeof ogType !== 'undefined' ? ogType : 'website' %>">
<meta property="og:site_name" content="Дача Зейна">
<meta property="og:url" content="https://dachazeyna.com<%= typeof currentPath !== 'undefined' ? currentPath : '' %>">
<meta property="og:title" content="<%= typeof title !== 'undefined' ? title : 'Дача Зейна' %>">
<meta property="og:description" content="<%= typeof description !== 'undefined' ? description : 'Сообщество Bee Swarm Simulator: гайды, торговля, экономика.' %>">
<meta property="og:image" content="<%= typeof image !== 'undefined' ? image : 'https://dachazeyna.com/assets/img/og-image.png' %>">
<meta property="og:locale" content="ru_RU">
<meta name="twitter:card" content="summary_large_image">

<link rel="icon" type="image/png" href="/assets/img/favicons/favicon-96x96.png" sizes="96x96" />
<link rel="icon" type="image/svg+xml" href="/assets/img/favicons/favicon.svg" />
<link rel="shortcut icon" href="/assets/img/favicons/favicon.ico" />
<link rel="apple-touch-icon" sizes="180x180" href="/assets/img/favicons/apple-touch-icon.png" />
<meta name="apple-mobile-web-app-title" content="Дача Зейна" />
<link rel="manifest" href="/assets/img/favicons/site.webmanifest" />

<link rel="preload" href="/assets/fonts/web/montserrat-v31-cyrillic_latin-regular.woff2" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="/assets/fonts/web/montserrat-v31-cyrillic_latin-900.woff2" as="font" type="font/woff2" crossorigin>

<style>
    /* Локальные шрифты */
    @font-face { font-family: 'Montserrat'; font-style: normal; font-weight: 400; font-display: swap; src: url('/assets/fonts/web/montserrat-v31-cyrillic_latin-regular.woff2') format('woff2'); }
    @font-face { font-family: 'Montserrat'; font-style: normal; font-weight: 500; font-display: swap; src: url('/assets/fonts/web/montserrat-v31-cyrillic_latin-500.woff2') format('woff2'); }
    @font-face { font-family: 'Montserrat'; font-style: normal; font-weight: 700; font-display: swap; src: url('/assets/fonts/web/montserrat-v31-cyrillic_latin-700.woff2') format('woff2'); }
    @font-face { font-family: 'Montserrat'; font-style: normal; font-weight: 900; font-display: swap; src: url('/assets/fonts/web/montserrat-v31-cyrillic_latin-900.woff2') format('woff2'); }
    
    /* ИСПРАВЛЕНИЕ LCP ДЛЯ ИКОНОК
       Переопределяем шрифты FontAwesome с font-display: swap 
       Ссылки ведут на тот же CDN, который разрешен в CSP.
    */
    @font-face {
        font-family: 'Font Awesome 6 Free';
        font-style: normal;
        font-weight: 900;
        font-display: swap; /* Самое важное свойство */
        src: url(https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/webfonts/fa-solid-900.woff2) format("woff2");
    }
    
    @font-face {
        font-family: 'Font Awesome 6 Free';
        font-style: normal;
        font-weight: 400;
        font-display: swap;
        src: url(https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/webfonts/fa-regular-400.woff2) format("woff2");
    }
    
    @font-face {
        font-family: 'Font Awesome 6 Brands';
        font-style: normal;
        font-weight: 400;
        font-display: swap;
        src: url(https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/webfonts/fa-brands-400.woff2) format("woff2");
    }

    .modal-overlay { display: none; opacity: 0; pointer-events: none; }
    .modal-overlay.active { display: flex; opacity: 1; pointer-events: all; }
</style>

<link rel="preload" href="/css/common.css" as="style" onload="this.onload=null;this.rel='stylesheet'">
